@model JobStack.WebUI.Areas.Manage.ViewModels.Country.CountryDto
@{
    ViewData["Title"] = "Create";
    Layout = "~/Areas/Manage/Views/Shared/_ManageLayout.cshtml";
}

<div class="container-fluid">
    <form asp-action="Create">
        <div class="form-row">
            <div class="form-group col-md-6">
                <label asp-for="CountryName">Country Name</label>
                <input asp-for="CountryName" class="form-control" placeholder=" Country Name">
            </div>


        </div>
        
        <div class="form-group">
            <button type="submit" class="btn btn-primary">Create</button>
        </div>
    </form>
</div>

